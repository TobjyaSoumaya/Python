        "def intersection(ensemble1, ensemble2):\n",
        "\n",
        "    return ensemble1.intersection(ensemble2)\n",
        "\n",
        "# Tests\n",
        "print(intersection({1, 2, 3}, {2, 3, 4}))\n",
        "print(intersection({\"a\", \"b\", \"c\"}, {\"c\", \"d\", \"e\"}))\n",
        "print(intersection({1, 2}, {3, 4}))\n",
        "print(intersection(set(), {1, 2, 3}))\n"
      ]
